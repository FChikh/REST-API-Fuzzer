#%RAML 1.0 Library

uses:
  error: !include ../schemas/error.raml

types:
  ReasonBadLicenseKey:
    description: 'Bad license key'
    type: error.error
    properties:
      domain:
        type: string
        enum:
          - Licensing
        required: true
      reason:
        type: string
        enum:
          - ReasonBadLicenseKey
        required: false
      code:
        type: string
        enum:
          - ReasonBadLicenseKey
        required: true
